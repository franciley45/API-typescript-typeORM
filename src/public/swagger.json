{
	"openapi": "3.0.0",
	"info": {
		"title": "API-TYPEORM.",
		"description": "Esta API foi criada com o intuito de aprimorar minhas habilidades em TypeScript, TypeORM, migrations, seeds, swagger, redocly e deploy, com o CRUD completo.Trata-se de uma playlist de filmes, onde as pessoas podem cadastrar novos filmes. Para isso, é necessário estar logado. Se você preferir, também pode utilizar a documentação feita com Redocly [http://localhost:3000/docs](http://localhost:3000/docs). Repositório [https://github.com/franciley45/API-typescript-typeORM](https://github.com/franciley45/API-typescript-typeORM)" ,
		"contact": {
			"email": "testepa45@gmail.com"
		},
		"version": "1.0.0"
	},
	"servers": [
		{
			"url": "https://api-typescript-type-orm-franciley45.vercel.app",
			"description": "API de Produção"
		},
		{
			"url": "http://localhost:3000",
			"description": "API de localhost"
		}
	],
	"paths": {
		"/users/login": {
			"post": {
				"summary": "Login de Usuário",
				"description": "Essa rota será responsável por Login do usuário",
				"tags": [
					"User"
				],
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/login"
							},
							"examples": {
								"user": {
									"value": {
										"email": "franciley@email.com",
										"password": "12345"
									}
								}
							}
						}
					}
				},
				"responses": {
					"400": {
						"description": "fill in all fields!"
					},
					"403": {
						"description": "unregistered user!"
					},
					"401": {
						"description": "wrong password!"
					},
					"200": {
						"description": "OK",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/user_return_post"
								}
							}
						}
					}
				}
			}
		},
		"/users": {
			"post": {
				"summary": "Cadastro de Usuário",
				"description": "Essa rota será responsável por cadastrar um novo usuário",
				"tags": [
					"User"
				],
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/user"
							},
							"examples": {
								"user": {
									"value": {
										"name": "franciley rodrigues",
										"email": "test@gmail.com",
										"password": "12345"
									}
								}
							}
						}
					}
				},
				"responses": {
					"400": {
						"description": "fill in all fields!"
					},
					"409": {
						"description": "User already registered!"
					},
					"201": {
						"description": "OK",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/user_return_post"
								}
							}
						}
					}
				}
			},
			"get": {
				"summary": "Busca todos os Usuários.",
				"description": "Essa rota será responsável por busca todos os usuários.",
				"tags": [
					"User"
				],
				"responses": {
					"200": {
						"description": "OK",
						"content": {
							"application/json": {
								"schema": {
									"type": "array",
									"items": {
										"$ref": "#/components/schemas/user"
									}
								}
							}
						}
					}
				}
			}
		},
		"/categories": {
			"get": {
				"summary": "Busca todos os videos.",
				"description": "Essa rota será responsável por busca todos os videos",
				"tags": [
					"Category"
				],
				"security": [
					{
						"bearerAuth": []
					}
				],
				"responses": {
					"200": {
						"description": "OK",
						"content": {
							"application/json": {
								"schema": {
									"type": "array",
									"items": {
										"$ref": "#/components/schemas/category"
									}
								}
							}
						}
					}
				}
			},
			"post": {
				"summary": "Cria uma nova Categoria",
				"description": "Essa rota será responsável por cria uma nova categoria.",
				"tags": [
					"Category"
				],
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/category"
							},
							"examples": {
								"Category": {
									"value": {
										"name": "Drama",
										"description": "filmes dramaticos"
									}
								}
							}
						}
					}
				},
				"security": [
					{
						"bearerAuth": []
					}
				],
				"responses": {
					"200": {
						"description": "OK",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/category"
								}
							}
						}
					},
					"401": {
						"description": "Expired or invalid token!"
					},
					"400": {
						"description": "Token not found!"
					},
					"409": {
						"description": "Category already exists"
					}
				}
			}
		},
		"/categories/{id}": {
			"delete": {
				"summary": "Deleta Categoria por Id",
				"description": "Essa rota será responsável por Deleta Categoria por Id.",
				"tags": [
					"Category"
				],
				"security": [
					{
						"bearerAuth": []
					}
				],
				"parameters": [
					{
						"name": "id",
						"in": "path",
						"description": "Id do Categoria para busca",
						"required": true,
						"schema": {
							"type": "string",
							"format": "int64"
						}
					}
				],
				"responses": {
					"401": {
						"description": "Expired or invalid token!"
					},
					"400": {
						"description": "Token not found!"
					},
					"402": {
						"description": "this Categoria is not yours!"
					}
				}
			},
			"put": {
				"summary": "Atualizar Categoria por Id",
				"description": "Essa rota será responsável por Atualizar Categoria por Id.",
				"tags": [
					"Category"
				],
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/category"
							},
							"examples": {
								"Category": {
									"value": {
										"name": "Drama",
										"description": "filmes de drama"
									}
								}
							}
						}
					}
				},
				"security": [
					{
						"bearerAuth": []
					}
				],
				"parameters": [
					{
						"name": "id",
						"in": "path",
						"description": "Id do Post para busca",
						"required": true,
						"schema": {
							"type": "string",
							"format": "int64"
						}
					}
				],
				"responses": {
					"200": {
						"description": "OK",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/category"
								}
							}
						}
					},
					"401": {
						"description": "Expired or invalid token!"
					},
					"400": {
						"description": "Token not found!"
					},
					"402": {
						"description": "The category does not exist"
					}
				}
			}
		},
		"/videos": {
			"get": {
				"summary": "Busca todos os Videos.",
				"description": "Essa rota será responsável por busca todos os videos",
				"tags": [
					"Video"
				],
				"security": [
					{
						"bearerAuth": []
					}
				],
				"responses": {
					"200": {
						"description": "OK",
						"content": {
							"application/json": {
								"schema": {
									"type": "array",
									"items": {
										"$ref": "#/components/schemas/video_return_get"
									}
								}
							}
						}
					}
				}
			},
			"post": {
				"summary": "Cria um novo Video",
				"description": "Essa rota será responsável por cria um novo video.",
				"tags": [
					"Video"
				],
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/video"
							},
							"examples": {
								"Video": {
									"value": {
										"name": "Todo mundo em Panico3",
										"description": "filme de besteirol",
										"duration": 120,
										"category_id": "copie o ID da categoria apage esse texto é cole aqui"
									}
								}
							}
						}
					}
				},
				"security": [
					{
						"bearerAuth": []
					}
				],
				"responses": {
					"201": {
						"description": "OK",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/video"
								}
							}
						}
					},
					"401": {
						"description": "Expired or invalid token!"
					},
					"400": {
						"description": "Token not found!"
					},
					"409": {
						"description": "Category already exists"
					}
				}
			}
		}
	},
	"components": {
		"schemas": {
			"user": {
				"type": "object",
				"properties": {
					"user_id": {
						"type": "string"
					},
					"name": {
						"type": "string"
					},
					"email": {
						"type": "string"
					},
					"password": {
						"type": "string"
					}
				}
			},
			"user_return_post": {
				"type": "object",
				"properties": {
					"user_id": {
						"type": "string"
					},
					"name": {
						"type": "string"
					},
					"token": {
						"type": "string"
					}
				}
			},
			"login": {
				"type": "object",
				"properties": {
					"email": {
						"type": "string"
					},
					"password": {
						"type": "string"
					}
				}
			},
			"category": {
				"type": "object",
				"properties": {
					"category_id": {
						"type": "string"
					},
					"name": {
						"type": "string"
					},
					"description": {
						"type": "string"
					},
					"created_at": {
						"type": "string"
					}
				}
			},
			"video_return_get": {
				"type": "object",
				"properties": {
					"video_id": {
						"type": "string"
					},
					"name": {
						"type": "string"
					},
					"description": {
						"type": "string"
					},
					"created_at": {
						"type": "string"
					},
					"duration": {
						"type": "number"
					},
					"category_id": {
						"type": "string"
					},
					"category": {
							"$ref": "#/components/schemas/category"
						}
					}
			},
			"video": {
				"type": "object",
				"properties": {
					"video_id": {
						"type": "string"
					},
					"name": {
						"type": "string"
					},
					"description": {
						"type": "string"
					},
					"created_at": {
						"type": "string"
					},
					"duration": {
						"type": "number"
					},
					"category_id": {
						"type": "string"
					}
				}
			}
		},
		"securitySchemes": {
			"bearerAuth": {
				"type": "http",
				"scheme": "bearer",
				"bearerFormat": "JWT"
			}
		}
	}
}